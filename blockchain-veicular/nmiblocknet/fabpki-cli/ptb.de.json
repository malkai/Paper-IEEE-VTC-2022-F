      "clientKey": {
        "path": "../crypto-config/peerOrganizations/ptb.de/peers/peer0.ptb.de/tls/server.key"
      },
      "clientCert": {
        "path": "../crypto-config/peerOrganizations/ptb.de/peers/peer0.ptb.de/tls/server.crt"
